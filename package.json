{
	"name": "typedoc-plugin-external-link",
	"version": "2.1.2",
	"description": "A typedoc plugin to add custom external links.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"license": "MIT",
	"author": "@imranbarbhuiya",
	"scripts": {
		"docs": "typedoc",
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"update": "yarn upgrade-interactive",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"prepack": "yarn build && pinst --disable",
		"postinstall": "husky .github/husky",
		"postpack": "pinst --enable"
	},
	"keywords": [
		"typedoc",
		"typedocplugin",
		"typedoc-plugin",
		"typescript",
		"ts",
		"yarn",
		"external-link",
		"link",
		"dependencies"
	],
	"devDependencies": {
		"@commitlint/cli": "^18.6.1",
		"@commitlint/config-conventional": "^18.6.2",
		"@favware/cliff-jumper": "^2.2.3",
		"@favware/npm-deprecate": "^1.0.7",
		"@types/node": "^20.11.19",
		"@typescript-eslint/eslint-plugin": "^7.0.1",
		"@typescript-eslint/parser": "^7.0.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.56.0",
		"eslint-config-mahir": "^0.0.43",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"pinst": "^3.0.0",
		"prettier": "^3.2.5",
		"tsup": "^8.0.2",
		"typedoc": "^0.25.8",
		"typescript": "^5.3.3"
	},
	"peerDependencies": {
		"typedoc": ">=0.24.0"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imranbarbhuiya/typedoc-plugin-external-link.git"
	},
	"bugs": {
		"url": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link/issues"
	},
	"homepage": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link#readme",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"packageManager": "yarn@4.1.0"
}
