{
	"name": "typedoc-plugin-external-link",
	"version": "2.0.0",
	"description": "A typedoc plugin to add custom external links.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"license": "MIT",
	"author": "@imranbarbhuiya",
	"scripts": {
		"docs": "typedoc",
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write \"{src,tests}/**/*.ts\"",
		"test": "vitest run",
		"test:watch": "vitest",
		"update": "yarn upgrade-interactive",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"prepack": "yarn build && pinst --disable",
		"postinstall": "husky install .github/husky",
		"postpack": "pinst --enable"
	},
	"keywords": [
		"typedoc",
		"typedocplugin",
		"typedoc-plugin",
		"typescript",
		"ts",
		"yarn",
		"external-link",
		"link",
		"dependencies"
	],
	"devDependencies": {
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@favware/cliff-jumper": "^2.1.1",
		"@favware/npm-deprecate": "^1.0.7",
		"@sapphire/eslint-config": "^4.4.2",
		"@sapphire/prettier-config": "^1.4.5",
		"@types/node": "^18.16.18",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"@typescript-eslint/parser": "^5.60.0",
		"@vitest/coverage-v8": "^0.32.2",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.43.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"pinst": "^3.0.0",
		"prettier": "^2.8.8",
		"pretty-quick": "^3.1.3",
		"tsup": "^7.0.0",
		"typedoc": "^0.24.8",
		"typescript": "^5.1.3",
		"vitest": "^0.32.2"
	},
	"peerDependencies": {
		"typedoc": ">=0.24.0"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imranbarbhuiya/typedoc-plugin-external-link.git"
	},
	"bugs": {
		"url": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link/issues"
	},
	"homepage": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link#readme",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.8"
	},
	"packageManager": "yarn@3.6.0",
	"prettier": "@sapphire/prettier-config"
}
