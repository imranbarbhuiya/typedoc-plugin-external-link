{
	"name": "typedoc-plugin-external-link",
	"version": "1.0.3",
	"description": "A typedoc plugin to add custom external links.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"sideEffects": false,
	"license": "MIT",
	"author": "@imranbarbhuiya",
	"scripts": {
		"docs": "typedoc",
		"lint": "eslint src tests --ext ts --fix",
		"format": "prettier --write \"{src,tests}/**/*.ts\"",
		"test": "vitest run",
		"test:watch": "vitest",
		"update": "yarn upgrade-interactive",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"prepack": "yarn build && pinst --disable",
		"postinstall": "husky install .github/husky",
		"postpack": "pinst --enable"
	},
	"keywords": [
		"typedoc",
		"typedocplugin",
		"typedoc-plugin",
		"typescript",
		"ts",
		"yarn",
		"external-link",
		"link",
		"dependencies"
	],
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@favware/cliff-jumper": "^1.8.8",
		"@favware/npm-deprecate": "^1.0.5",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.4",
		"@types/node": "^18.6.1",
		"@typescript-eslint/eslint-plugin": "^5.40.1",
		"@typescript-eslint/parser": "^5.40.1",
		"@vitest/coverage-c8": "^0.24.3",
		"c8": "^7.12.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"pinst": "^3.0.0",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"tsup": "^6.3.0",
		"typedoc": "^0.23.17",
		"typescript": "^4.8.4",
		"vitest": "^0.24.3"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imranbarbhuiya/typedoc-plugin-external-link.git"
	},
	"bugs": {
		"url": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link/issues"
	},
	"homepage": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link#readme",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"resolutions": {
		"ansi-regex": "^5.0.1",
		"minimist": "^1.2.7"
	},
	"packageManager": "yarn@3.2.4",
	"prettier": "@sapphire/prettier-config"
}
