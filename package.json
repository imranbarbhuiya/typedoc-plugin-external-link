{
	"name": "typedoc-plugin-external-link",
	"version": "3.0.2",
	"description": "A typedoc plugin to add custom external links.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"sideEffects": false,
	"license": "MIT",
	"author": "@imranbarbhuiya",
	"scripts": {
		"docs": "typedoc",
		"lint": "eslint . --cache",
		"format": "prettier --write . --cache",
		"build": "tsup",
		"typecheck": "tsc -p tsconfig.json",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run",
		"prepack": "yarn build && pinst --disable",
		"postinstall": "husky .github/husky",
		"postpack": "pinst --enable"
	},
	"keywords": [
		"typedoc",
		"typedocplugin",
		"typedoc-plugin",
		"typescript",
		"ts",
		"yarn",
		"external-link",
		"link",
		"dependencies"
	],
	"devDependencies": {
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@favware/cliff-jumper": "^6.0.0",
		"@favware/npm-deprecate": "^2.0.0",
		"@types/node": "^22.15.21",
		"@typescript-eslint/eslint-plugin": "^8.32.1",
		"@typescript-eslint/parser": "^8.32.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.27.0",
		"eslint-config-mahir": "^1.0.7",
		"husky": "^9.1.7",
		"lint-staged": "^16.0.0",
		"pinst": "^3.0.0",
		"prettier": "^3.5.3",
		"tsup": "^8.5.0",
		"typedoc": "^0.28.4",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"typedoc": ">=0.27.0"
	},
	"files": [
		"dist/**/*.js*",
		"dist/**/*.mjs*",
		"dist/**/*.d*"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/imranbarbhuiya/typedoc-plugin-external-link.git"
	},
	"bugs": {
		"url": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link/issues"
	},
	"homepage": "https://github.com/imranbarbhuiya/typedoc-plugin-external-link#readme",
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"packageManager": "yarn@4.9.1"
}
